{
    "default": "terminal",
    "terminal": {
        "async_mode": false,
        "color": true,
        "format": "[{TIME}] [{FILE:short}] [{FUNCTION}:{LINE}] [{TAG}] {MESSAGE}",
        "severity_level": 31,
        "stream": "stdout",
        "time_format": "{YEAR}-{MONTH:numeric}-{DAY_MONTH} {HOUR:24}:{MINUTE}:{SECOND}.{MILLISECOND}",
        "type": "terminal"
    }
}